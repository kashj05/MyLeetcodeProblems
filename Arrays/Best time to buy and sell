class Solution {
    public int maxProfit(int[] prices) {
        if(prices.length==0){
            return 0;
        }
        int profit=0;
        int min=prices[0];
        int maxprofit=0;
        for(int stockprices:prices){
            min=Math.min(min,stockprices);
            profit=stockprices-min;
            maxprofit=Math.max(profit,maxprofit);
        }
        return maxprofit;
      }
        
    }
